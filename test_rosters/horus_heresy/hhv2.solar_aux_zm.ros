        <category id="3c39-0319-53e6-6d4f" name="LoW &amp; Primarchs (25% Limit)" entryId="2eaf-32d6-9d1d-d906" primary="false"/>
        <category id="da5b-c90b-fbe8-d07b" name="Compulsory HQ:" entryId="f823-8c1d-6a87-26a1" primary="false"/>
