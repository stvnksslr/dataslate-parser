          <selections>
            <selection id="b39d-a831-ef28-2520" name="Misericord" entryId="608e-6aee-421f-3e30::15ed-0168-e84d-4b56::73cb-1501-d39e-4f24" number="1" type="upgrade">
              <rules>
                <rule id="608e-6aee-421f-3e30::15ed-0168-e84d-4b56::face-fe0e-ad8a-3a98::9e96-fff1-b916-d9a3" name="Instant Death" publicationId="e77a-823a-da94-16b9" page="242" hidden="false">
                  <description>If a model suffers an unsaved Wound from an attack with this special rule, it is reduced to 0 Wounds and is removed as a casualty.</description>
                </rule>
              </rules>
              <profiles>
                <profile id="608e-6aee-421f-3e30::15ed-0168-e84d-4b56::df49-46f1-e6c4-d83e" name="Misericord" hidden="false" typeId="1a1a-e592-2849-a5c0" typeName="Weapon">
